<Window x:Class="converter_coords.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:converter_coords"
        mc:Ignorable="d"
        Title="Konwerter Koordow" Height="800" Width="500">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="80*"/>
            <ColumnDefinition Width="250*"/>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="50"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <Button Content="Konwerter Koordów" HorizontalAlignment="Center" Margin="0,25,0,0" Grid.Row="0" VerticalAlignment="Top" Click="Button_Click"/>
        <Button Content="Elo mordo" HorizontalAlignment="Center" Margin="0,25,0,0" Grid.Row="1" VerticalAlignment="Top"/>
        <Button Content="Ładna dziś pogoda" HorizontalAlignment="Center" Margin="0,25,0,0" Grid.Row="2" VerticalAlignment="Top"/>
        <Frame x:Name="frame" Content="" Grid.RowSpan="3" Grid.Column="2" Height="500"/>

    </Grid>

</Window>
